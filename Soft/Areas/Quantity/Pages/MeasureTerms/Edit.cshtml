@page
@using Abc.Pages.Extensions
@model Abc.Soft.Areas.Quantity.Pages.MeasureTerms.EditModel

@{
    Layout = "_Edit";
    var itemId = Model.ItemId;

}

<body>
    <input type="hidden" asp-for="@itemId" />
    @Html.EditControlsForDropDown(m => m.Item.MasterId, Model.Measures)
    @Html.EditControlsForDropDown(m => m.Item.TermId, Model.Measures)
    @Html.EditControlsFor(m => m.Item.Power)
    @Html.EditControlsFor(m => m.Item.ValidFrom)
    @Html.EditControlsFor(m => m.Item.ValidTo)
</body>

